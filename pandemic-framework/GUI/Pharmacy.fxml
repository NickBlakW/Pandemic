<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.PharmacyController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Pharmacy.png" />
         </image></ImageView>
      <Group layoutX="303.0" layoutY="306.0">
         <children>
            <ImageView fx:id="imgView" fitHeight="42.0" fitWidth="45.0" layoutX="1.0" layoutY="-20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/Pills.png" />
               </image>
            </ImageView>
            <Button fx:id="itemButton" layoutX="9.0" layoutY="-22.0" mnemonicParsing="false" onMouseClicked="#takeItem" opacity="0" prefHeight="46.0" prefWidth="27.0" text="Button" />
         </children>
      </Group>
      <Button fx:id="upButton" layoutX="349.0" layoutY="1.0" mnemonicParsing="false" onMouseClicked="#buttonAction" opacity="0.0" prefHeight="47.0" prefWidth="101.0" text="Cafeteria" />
      <Button fx:id="rightButton" layoutX="751.0" layoutY="348.0" mnemonicParsing="false" onMouseClicked="#buttonAction" opacity="0.0" prefHeight="102.0" prefWidth="48.0" text="Reception" />
      <Button fx:id="inventoryButton" layoutX="660.0" layoutY="700.0" mnemonicParsing="false" onMouseClicked="#openInventory" prefHeight="37.0" prefWidth="83.0" text="Inventory" />
   </children>
</AnchorPane>
