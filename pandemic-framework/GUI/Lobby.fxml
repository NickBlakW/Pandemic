<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainRoot" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LobbyController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Lobby.png" />
         </image></ImageView>
      <Button fx:id="leftButton" layoutY="218.0" mnemonicParsing="false" onMouseClicked="#buttonAction" opacity="0.0" prefHeight="59.0" prefWidth="33.0" text="Reception" />
      <Button fx:id="inventoryButton" layoutX="403.0" layoutY="438.0" mnemonicParsing="false" onMouseClicked="#goToInventory" text="Inventory" />
   </children>
</AnchorPane>
