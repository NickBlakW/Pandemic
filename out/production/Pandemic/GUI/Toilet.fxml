      <Text layoutX="289.0" layoutY="196.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Toilet" />
      <ImageView fx:id="cake" fitHeight="150.0" fitWidth="200.0" layoutX="26.0" layoutY="82.0" onMouseClicked="#pickUpSoap" pickOnBounds="true" preserveRatio="true">
