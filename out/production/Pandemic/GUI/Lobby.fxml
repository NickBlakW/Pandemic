<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="mainRoot" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LobbyController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Lobby.png" />
         </image></ImageView>
      <Button fx:id="leftButton" layoutX="14.0" layoutY="236.0" mnemonicParsing="false" onMouseClicked="#buttonAction" text="Reception" />
      <Button fx:id="inventoryButton" layoutX="317.0" layoutY="353.0" mnemonicParsing="false" onMouseClicked="#goToInventory" text="Inventory" />
      <Text layoutX="34.0" layoutY="18.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Lobby" />
   </children>
</AnchorPane>
