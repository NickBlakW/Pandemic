   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Lobby.png" />
         </image></ImageView>
      <Button fx:id="leftButton" layoutX="14.0" layoutY="188.0" mnemonicParsing="false" onMouseClicked="#buttonAction" text="Reception" />
      <Button fx:id="inventoryButton" layoutX="317.0" layoutY="353.0" mnemonicParsing="false" onMouseClicked="#goToInventory" text="Inventory" />
      <TextField layoutX="67.0" layoutY="32.0" text="Lobby" />
   </children>
</AnchorPane>
