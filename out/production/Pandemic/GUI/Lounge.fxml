<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LoungeController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Lounge.png" />
         </image>
      </ImageView>
      <ImageView fx:id="plant" fitHeight="86.0" fitWidth="106.0" layoutX="671.0" layoutY="464.0" onMouseClicked="#takeItem" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Plant.png" />
         </image>
      </ImageView>
      <Button fx:id="leftButton" layoutY="348.0" mnemonicParsing="false" onMouseClicked="#buttonAction" opacity="0.0" prefHeight="103.0" prefWidth="50.0" text="Cafeteria" />
      <Button fx:id="downButton" layoutX="347.0" layoutY="750.0" mnemonicParsing="false" onMouseClicked="#buttonAction" opacity="0.0" prefHeight="49.0" prefWidth="106.0" text="Reception" />
      <Button fx:id="rightButton" layoutX="751.0" layoutY="348.0" mnemonicParsing="false" onMouseClicked="#buttonAction" opacity="0.0" prefHeight="103.0" prefWidth="50.0" text="South Hallway" />
      <Button fx:id="inventoryButton" layoutX="660.0" layoutY="700.0" mnemonicParsing="false" onMouseClicked="#openInventory" prefHeight="37.0" prefWidth="83.0" text="Inventory" />
   </children>
</AnchorPane>
