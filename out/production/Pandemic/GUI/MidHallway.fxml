<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.MidHallwayController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Midhallway.png" />
         </image>
      </ImageView>
      <ImageView fx:id="mask" fitHeight="36.0" fitWidth="40.0" layoutX="333.0" layoutY="309.0" onMouseClicked="#takeItem" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Mask.png" />
         </image>
      </ImageView>
      <Button fx:id="upButton" layoutX="351.0" mnemonicParsing="false" onMouseClicked="#buttonAction" opacity="0.0" prefHeight="48.0" prefWidth="97.0" text="North Hallway" />
      <Button fx:id="downButton" layoutX="351.0" layoutY="699.0" mnemonicParsing="false" onMouseClicked="#buttonAction" opacity="0.0" prefHeight="103.0" prefWidth="97.0" text="South Hallway" />
      <Button fx:id="inventoryButton" layoutX="660.0" layoutY="650.0" mnemonicParsing="false" onMouseClicked="#openInventory" prefHeight="37.0" prefWidth="83.0" text="Inventory" />
   </children>
</AnchorPane>
